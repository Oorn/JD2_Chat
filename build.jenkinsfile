pipeline {
  agent any
  stages {
    stage('Build Project') {
        steps {
            sh 'mvn clean'
	        sh 'mvn install'
            sh 'mvn spring-boot:repackage'
        }
    }
    stage('Deploy Project') {
        steps {
            build wait: false, job: 'deploy'
        }
    }
  }
}
